{
    "name": "typescript-playground",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev": "turbo run dev",
        "dev:gh-pages": "turbo run dev --filter=@typescript-playground/gh-pages",
        "dev:playground": "turbo run dev --filter=@typescript-playground/playground",
        "build": "turbo run build",
        "test": "turbo run test",
        "lint": "turbo run lint"
    },
    "devDependencies": {
        "turbo": "^2.7.3",
        "typescript": "^5.9.3"
    },
    "packageManager": "npm@11.6.2"
}